@model YouthGovence.Models.SingleEvent

@{
    ViewBag.Title = "SingleEvent";
    Layout = "~/Views/Shared/_VolunteerLayout.cshtml";
}

<h2>SingleEvent</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>StudentParticipation</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        

        <div class="form-group">
            @Html.LabelFor(model => model.SinEvtID, "SinEvtID", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("SinEvtID", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.SinEvtID, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
