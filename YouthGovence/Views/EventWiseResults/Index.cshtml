@model IEnumerable<YouthGovence.Models.EventWiseResult>

@{
    ViewBag.Title = "Event Wise Result";
    Layout = "~/Views/Shared/_VolunteerLayout.cshtml";
}

@section Styles {
    <!-- Custom styles for this page -->
    <link href="~/Content/Admin/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

<p>
    <a href="/News/Create"><button class="btn btn-primary btn-user btn-block">Create New News</button></a>
</p>


<div class="card-body">
    <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.EventName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FStudChestNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FStudName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FStudCollege)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SStudChestNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SStudName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SStudCollege)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TStudChestNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TStudName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TStudCollege)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EnteredOn)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.UpdatedOn)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Volunteer.Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Volunteer1.Name)
                    </th>
                    <th></th>
                </tr>

            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.EventName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FStudChestNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FStudName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FStudCollege)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SStudChestNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SStudName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SStudCollege)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TStudChestNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TStudName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TStudCollege)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EnteredOn)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UpdatedOn)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Volunteer.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Volunteer1.Name)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.EvntID }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.EvntID })
                        </td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>
@section scripts
{
    <!-- Page level plugins -->
    <script src="~/Scripts/Admin/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/Admin/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Scripts/Admin/datatables-demo.js"></script>
}
